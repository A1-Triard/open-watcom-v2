proj_name = watcom

watcom_autodepends = .autodepend

!ifeq sharedlib 1
sys_dll = 1
objs     = $(dll_subdir)/clibext.obj
lib_name = clibexts.lib
!else
objs     = clibext.obj
lib_name = clibext.lib
!endif

!include cproj.mif
!include defrule.mif
!include deftarg.mif

.c: ../c

inc_dirs = -I"../h"

!ifdef __WATCOM__
skip : .symbolic
!else
$(lib_name) : $(objs)
    $(bld_lib) $<
!endif
