pfx__    =$(cpplib_dir)
__ifx__  =$(system).386/$(model)_$(dllname_$(convention))mt$(dllname_profile)$(dllname_debug)
__cifx__ =$(system).386/$(subdir)
__mifx__ =$(system).387/$(subdir)

dll_libs = $+ &
    $(pfx__)/library/$(__ifx__).dll/plbxmt3$(dllname_$(convention)).lib &
    $(clib_dir)/rtdll/$(__cifx__)/clb$(dllname_$(convention))dll$(dllname_profile)$(dllname_debug).lib &
    $(mathlib_dir)/rtdll/$(__mifx__)/mt7$(dllname_$(convention))dll$(dllname_profile)$(dllname_debug).lib &
    $(pfx__)/library/$(__ifx__)/plbxmt3$(dllname_$(convention)).lib &
    $-

dll_def = $+ &
    $(pfx__)/runtime/os2386.def &
    $(pfx__)/library/$(__ifx__).dll/exportx.def &
    $-

dll_objs = $+ &
    $(clib_dir)/startup/library/$(__cifx__)/clibdo32.obj &
    $-

targdir = $(pfx__)/runtime/$(__ifx__).dll/
!include $(pfx__)/runtime/static.mif
objs = $+ $(static_objs) $-
targdir = $(pfx__)/iostream/$(__ifx__).dll/
!include $(pfx__)/iostream/static.mif
objs += $+ $(static_objs) $-
targdir =
static_objs = $+ $(objs) $-

pfx__          =
__ifx__        =
