proj_name = cwc

cwc_autodepends = .AUTODEPEND

!include cproj.mif
!include defrule.mif
!include deftarg.mif

.c : ../../c

extra_l_flags = op map

objs = cwc.obj

$(proj_name).exe : $(objs) $(cl_objs)
