proj_name = wlink

wlink_autodepends = .AUTODEPEND

memory_model_i86 = c

!include cproj.mif
!include defrule.mif
!include deftarg.mif

objs = &
fileio.obj &
keyword.obj &
mem.obj &
ms2wlink.obj &
parsems.obj &
utils.obj

.c: ../c

inc_dirs = -I"../h"

extra_l_flags_os2 = op stack=8k

ms2wlink.exe : $(objs)
!ifndef __WATCOM__
        $(cl) $(clflags) $(objs) $(ldflags)
!else
        $(linker) $(lflags) name $^@ op map file {$(objs)}
!endif
