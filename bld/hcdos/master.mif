proj_name = hcdos

hcdos_autodepends = .autodepend

hcdos_trmem = 1

!include cproj.mif
!include defrule.mif
!include deftarg.mif
!include trmem.mif

inc_dirs = -I"../h" -I"$(hlpview_dir)/h" -I"$(ui_dir)/h"

.c: ../c;$(hlpview_dir)/c

objs = &
    sorthelp.obj &
    index.obj &
    helpscan.obj &
    helpmem.obj

extra_cpp_flags_helpmem = $(trmem_cpp_flags)

extra_c_flags_helpmem = $(trmem_cover_cflags)

# Note: dmp.c can be built into a separate utility.

!include exetarg.mif
