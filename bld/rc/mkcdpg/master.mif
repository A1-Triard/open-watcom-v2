proj_name       = mkcdpg

mkcdpg_autodepends = .AUTODEPEND

inc_dirs        = -I"$(wrc_dir)/h"

!include cproj.mif
!include defrule.mif
!include deftarg.mif

.c: ..

objs = mkcdpg.obj

$(proj_name).exe : $(objs)
!ifndef __WATCOM__
        $(cl) $(clflags) $(objs) $(ldflags)
!else
        $(linker) $(lflags) name $^. file { $< }
!endif
