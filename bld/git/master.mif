proj_name = cpyright

watcom_autodepends = .autodepend

name = $(proj_name)

!include cproj.mif
!include defrule.mif
!include deftarg.mif

objs = $(proj_name).obj

.c: ../c

$(proj_name).exe : $(objs) $(__MAKEFILES__)
!ifndef __WATCOM_TOOLS__
        $(cl) $(clflags) $(objs) $(ldflags)
!else
        $(linker) name $@ $(lflags) file {$(objs)}
!endif
