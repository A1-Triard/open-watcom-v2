pfx__ =$(mathlib_dir)
__ifx__ =$(system).$(processor)/$(subdir)

dll_libs = $+ &
    $(pfx__)/library/$(__ifx__).dll/math3$(dllname_$(convention)).lib &
    $(clib_dir)/rtdll/$(__ifx__)/clb$(dllname_$(convention))dll$(dllname_profile)$(dllname_debug).lib &
    $-

dll_def = $+ &
    $(pfx__)/library/$(__ifx__).dll/export.def &
    $(pfx__)/export.$(processor) &
    $-

dll_objs = $+ &
    $(clib_dir)/startup/library/$(__ifx__)/clibdwnt.obj &
    $-

static_objs =

pfx__ =
__ifx__ =
