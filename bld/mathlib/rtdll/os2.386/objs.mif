pfx__ =$(mathlib_dir)
__ifx__ =$(system).$(processor)/$(subdir).dll

dll_libs = $+ &
    $(pfx__)/library/$(__ifx__)/math3$(dllname_$(convention)).lib &
    $(clib_dir)/rtdll/$(system).386/$(subdir)/clb$(dllname_$(convention))dll$(dllname_profile)$(dllname_debug).lib &
    $-

dll_def = $+ &
    $(pfx__)/library/$(__ifx__)/export.def &
    $(pfx__)/export.$(processor) &
    $-

dll_objs = $+ &
    $(clib_dir)/startup/library/$(system).386/$(subdir)/clibdo32.obj &
    $-

static_objs =

pfx__ =
__ifx__ =
