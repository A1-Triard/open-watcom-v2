[ DEFAULT desc="Debugger information strip utility" type="e" ]
dir="binw"      usr="wstrip.exe"    cond="doshost winhost |"    where="c jc f77 jf77"       old="binb"
dir="binp"      usr="wstrip.exe"    cond="os2host"              where="c jc f77 jf77"
dir="binnt"     usr="wstrip.exe"    cond="wnthost"              where="c jc f77 jf77 ac"
dir="binnt64"   usr="wstrip.exe"    cond="wnt64host"            where="c jc f77 jf77" keys="ntx64"
dir="axpnt"     usr="wstrip.exe"    cond="alphahost"            where="ac"
dir="binl"      usr="wstrip"        cond="lnxhost"              where="c jc f77 jf77"
dir="binl64"    usr="wstrip"        cond="lnx64host"            where="c jc f77 jf77" keys="lx64"
