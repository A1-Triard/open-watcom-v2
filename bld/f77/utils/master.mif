proj_name = errsrc

errsrc_autodepends = .autodepend

!include cproj.mif
!include defrule.mif
!include deftarg.mif

.c: ../c

errsrc.exe : errsrc.obj $(__MAKEFILES__)
    @%make echo_link
!ifdef bootstrap
    $(bld_cl) $(bld_clflags) errsrc.obj $(bld_ldflags)
!else ifndef __WATCOM_TOOLS__
    $(cl) $(clflags) errsrc.obj $(ldflags)
!else
    $(linker) name $@ $(lflags) file {errsrc.obj}
!endif
