proj_name = os2apitest
os2apitest_autodepends = .autodepend

all : testc.exe testpp.exe .symbolic

!include cproj.mif
!include deftarg.mif
!include defrule.mif

!ifeq host_cpu 386
inc_dirs = -I"../../os2386/h"
!else
inc_dirs = -I"../../os2286/h"
!endif

extra_c_flags = -D_ENABLE_AUTODEPEND

testc.exe : testc.obj $(libs) $(cl_objs)
    $(cl) $(clflags) $< $(cl_libs)

testpp.exe : testpp.obj $(libs) $(cl_objs)
    $(cl) $(clflags) $< $(cl_libs)
