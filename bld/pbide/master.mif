proj_name = pbide

pbide_autodepends = .AUTODEPEND

!include cproj.mif
!include defrule.mif
!include deftarg.mif

dlldbg.exe : dlldbg.obj dlldbg.res
        $(linker) op map $(lflags) name $^. file dlldbg.obj
        $(rc) -k dlldbg.res $^@

.c: ..

extra_c_flags_win =-zW

dlldbg.res : ../dlldbg.rc ../debugger.ico
        $(rc) $(rc_flags) $[@ -fo=$@
