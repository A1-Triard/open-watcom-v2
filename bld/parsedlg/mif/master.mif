proj_name = parsedlg

parsedlg_autodepends = .autodepend

!include cproj.mif
!include deftarg.mif
!include defrule.mif

objs = parsedlg.obj

inc_dirs = -I"../h" -I"$(watcom_dir)/h"

.c: ../c

$(proj_name).exe : $(objs)
!ifndef __WATCOM__
        $(cl) $(clflags) $< $(ldflags)
!else
        $(linker) name $@ $(lflags) file {$(objs)}
!endif
