clean: .SYMBOLIC .EXPLICIT
    rm -f *.obj $(additional_cleanup_$(host_os)) $(additional_cleanup)
    rm -rf $(clean_dll_subdir) $(additional_cleanup_subdir)
    @%make killnonobj

killnonobj: .SYMBOLIC .EXPLICIT
    rm -f *.lnd *.lnk *.exe *.exp *.nlm *.dll *.lib *.lbc *.gc *.gh *.grc
    rm -f *.res *.lst *.err *.pch *.map *.sym *.tmp *.trk $(additional_cleanup_sys)
